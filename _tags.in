<bdd-full/*>: for-pack(BDDFull)
<cnf/*>: for-pack(CNF)
<eq/*>: for-pack(Eq)
<logger/*>: for-pack(Logger)
<lin/*>: for-pack(Lin)
<packer/*>: for-pack(Packer)
<stats/*>: for-pack(Stats)
<union-find/*>: for-pack(UnionFind)
<sdsl/*>: for-pack(SDSL)
<sing/*>: for-pack(Sing)
<qbf/*>: for-pack(QBF)
<smt/*>: for-pack(SMT)
<quicg/*>: for-pack(QUICG)
<ds/*>: for-pack(DS)
<strace/*>: for-pack(STrace)
<tracer/*>: for-pack(Tracer)
<debugger/*>: for-pack(Debugger)
<Main.*>: package(str)
<*> or <stats/*>: package(unix)
#ifdef PKG_MLBDD
<*> or <bdd-full/*>: package(mlbdd)
#endif
#ifdef PKG_Z3
<Main.*> or <SMT.*> or <smt/*> or <QUICG.*> or <quicg/*>: package(Z3)
#endif
